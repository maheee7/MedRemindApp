{
    "crons": [
        {
            "path": "/api/cron/check-missed",
            "schedule": "*/10 * * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/(api/.*)",
            "destination": "/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}